<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.linyilinyi.mapper.FileMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.linyilinyi.entity.po.File">
        <id column="id" property="id" />
        <result column="file_name" property="fileName" />
        <result column="file_type" property="fileType" />
        <result column="bucket" property="bucket" />
        <result column="file_url" property="fileUrl" />
        <result column="file_path" property="filePath" />
        <result column="file_md5" property="fileMd5" />
        <result column="user_id" property="userId" />
        <result column="start" property="start" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
        <result column="id_delete" property="idDelete" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, file_name, file_type, bucket, file_url, file_path, file_md5, user_id, start, create_time, update_time, id_delete
    </sql>

</mapper>
